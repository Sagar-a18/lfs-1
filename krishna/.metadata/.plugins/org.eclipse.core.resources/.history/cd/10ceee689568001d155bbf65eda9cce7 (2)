package krishna;

import java.util.Scanner;

public class K{ 
	
	public static void main(String[]args) {
		System.out.println("enter thr number :");
		Scanner sc = new Scanner(System.in);
	 int a=sc.nextInt();
	 int rev=0;
	while(a>0) {
	rev =rev*10+a%10;
	a=a/10;
	System.out.println(rev);
	
	}

	}

}
